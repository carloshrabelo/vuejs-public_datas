      "node_modules/(?!vue-lottie)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  }
